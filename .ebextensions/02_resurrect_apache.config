container_commands:
  01_stop_apache:
    command: "killall httpd"
  02_remove_ssl_conf:
    command: "sudo rm /etc/httpd/conf.d/ssl.conf"
  03_undo_certbot_wsgi_changes:
    command: "mv /etc/httpd/conf.d/wsgi.conf.bkp /etc/httpd/conf.d/wsgi.conf"
  04_remove_certbot:
    command: "sudo rm -rf /opt/certbot && sudo rm -rf /opt/eff.org && yum remove mod24_ssl"
  05_restart_apache:
    command: "sudo restart supervisord || sudo start supervisord"
